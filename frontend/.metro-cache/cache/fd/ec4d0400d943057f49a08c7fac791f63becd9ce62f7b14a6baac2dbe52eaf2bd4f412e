: new Date().toISOString().split('T')[0],\n            amount: amount,\n            note: adjustmentNote || 'No note'\n          };\n          workerData.adjustmentEntries = [...(workerData.adjustmentEntries || []), adjustmentEntry];\n          workerData.otherAdjustments = (workerData.otherAdjustments || 0) + amount;\n          await AsyncStorage.default.setItem(`worker_${selectedWorker.id}`, JSON.stringify(workerData));\n          Alert.default.alert('Success', `Adjustment of ₹${amount} added for ${selectedWorker.name}`);\n          setAdjustmentModalVisible(false);\n          setAdjustmentAmount('');\n          setAdjustmentNote('');\n        }\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to add adjustment');\n      }\n    };\n\n    // Handle hide worker\n    const handleHideWorker = async workerId => {\n      await (0, _libStorage.updateWorkerHiddenStatus)(workerId, true);\n      setWorkers(prevWorkers => prevWorkers.map(worker => worker.id === workerId ? Object.assign({}, worker, {\n        hidden: true\n      }) : worker));\n      Alert.default.alert('Success', 'Worker hidden successfully!');\n    };\n\n    // Handle activate worker\n    const handleActivateWorker = async workerId => {\n      await (0, _libStorage.updateWorkerHiddenStatus)(workerId, false);\n      setWorkers(prevWorkers => prevWorkers.map(worker => worker.id === workerId ? Object.assign({}, worker, {\n        hidden: false\n      }) : worker));\n      Alert.default.alert('Success', 'Worker activated successfully!');\n    };\n\n    // Handle add worker\n    const handleAddWorker = newWorkerData => {\n      // For now, use a simple ID generation and add to the workers array\n      const newWorkerId = Math.max(...workers.map(w => parseInt(w.id))) + 1;\n      const newWorker = {\n        id: newWorkerId.toString(),\n        name: newWorkerData.name,\n        phone: newWorkerData.phone,\n        role: newWorkerData.role,\n        attendanceStatus: null,\n        overtime: 