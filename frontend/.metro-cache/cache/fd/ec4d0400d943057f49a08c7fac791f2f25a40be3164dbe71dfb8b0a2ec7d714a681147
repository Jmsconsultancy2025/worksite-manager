 () => {\n        const now = new Date();\n        const currentDate = now.toISOString().split('T')[0];\n        const lastCheckedDate = await AsyncStorage.default.getItem('lastAdvanceCheckDate');\n        if (lastCheckedDate !== currentDate) {\n          // Day has changed, reset todayAdvanceTotal for all workers\n          setWorkers(prevWorkers => prevWorkers.map(worker => Object.assign({}, worker, {\n            todayAdvanceTotal: 0\n          })));\n          await AsyncStorage.default.setItem('lastAdvanceCheckDate', currentDate);\n        }\n      };\n\n      // Check immediately and then every minute\n      checkDayChange();\n      const interval = setInterval(checkDayChange, 60000); // Check every minute\n\n      return () => clearInterval(interval);\n    }, []);\n\n    // Modal states\n    const [optionsModalVisible, setOptionsModalVisible] = (0, _react.useState)(false);\n    const [advanceModalVisible, setAdvanceModalVisible] = (0, _react.useState)(false);\n    const [overtimeModalVisible, setOvertimeModalVisible] = (0, _react.useState)(false);\n    const [adjustmentModalVisible, setAdjustmentModalVisible] = (0, _react.useState)(false);\n    const [selectedWorker, setSelectedWorker] = (0, _react.useState)(null);\n\n    // Advance form state\n    const [advanceAmount, setAdvanceAmount] = (0, _react.useState)('');\n    const [advanceDate, setAdvanceDate] = (0, _react.useState)(new Date().toISOString().split('T')[0]);\n\n    // Overtime form state\n    const [overtimeRate, setOvertimeRate] = (0, _react.useState)('');\n    const [overtimeHours, setOvertimeHours] = (0, _react.useState)('');\n\n    // Adjustment form state\n    const [adjustmentAmount, setAdjustmentAmount] = (0, _react.useState)('');\n    const [adjustmentNote, setAdjustmentNote] = (0, _react.useState)('');\n\n    // Add Worker Modal state\n    const [isAddWorkerModalOpen, setIsAddWorkerModalOpen] = (0, _react.useState)(false);\n\n    // Edit Worker Modal state\n    const [editModalVisible, setE