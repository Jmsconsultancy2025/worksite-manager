 to the workers array\n      const newWorkerId = Math.max(...workers.map(w => parseInt(w.id))) + 1;\n      const newWorker = {\n        id: newWorkerId.toString(),\n        name: newWorkerData.name,\n        phone: newWorkerData.phone,\n        role: newWorkerData.role,\n        attendanceStatus: null,\n        overtime: false,\n        maxAdvanceLimit: 5000,\n        // Default limit\n        hidden: false,\n        todayAdvanceTotal: 0,\n        dailyRate: newWorkerData.dailyRate,\n        site: newWorkerData.site || siteParam || 'All Sites'\n      };\n      setWorkers(prev => [...prev, newWorker]);\n      Alert.default.alert('Success', `Worker ${newWorkerData.name} added successfully!`);\n    };\n\n    // Get badge style based on status\n    const getBadgeStyle = (worker, badgeType) => {\n      const statusMap = {\n        P: 'present',\n        H: 'halfday',\n        A: 'absent'\n      };\n      const isActive = worker.attendanceStatus === statusMap[badgeType];\n      if (isActive) {\n        if (badgeType === 'P') return {\n          backgroundColor: '#4CAF50',\n          color: '#FFFFFF'\n        };\n        if (badgeType === 'H') return {\n          backgroundColor: '#FFC107',\n          color: '#FFFFFF'\n        };\n        if (badgeType === 'A') return {\n          backgroundColor: '#F44336',\n          color: '#FFFFFF'\n        };\n      }\n      return {\n        backgroundColor: '#E0E0E0',\n        color: '#757575'\n      };\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n      style: styles.safeArea,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(StatusBar.default, {\n        barStyle: \"dark-content\",\n        backgroundColor: \"#FFFFFF\"\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 605,\n        columnNumber: 7\n      }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.container,\n        children