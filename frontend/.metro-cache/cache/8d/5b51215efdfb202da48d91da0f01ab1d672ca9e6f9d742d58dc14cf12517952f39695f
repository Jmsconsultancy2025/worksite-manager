        phone: editPhone.trim(),\n          role: editRole.trim()\n        }) : worker);\n        setWorkers(updatedWorkers);\n\n        // Save updated list to AsyncStorage\n        await (0, _libStorage.saveWorkersList)(updatedWorkers);\n        Alert.default.alert('Success', 'Worker updated successfully!');\n        setEditModalVisible(false);\n        setEditingWorker(null);\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to update worker');\n      }\n    };\n\n    // Handle add worker\n    const handleAddWorker = async newWorkerData => {\n      // Check worker limit before adding\n      const canAddWorker = await (0, _libStorage.checkWorkerLimit)(workers.length);\n      if (!canAddWorker) {\n        const subscription = await (0, _libStorage.getUserSubscription)();\n        const planName = subscription.plan.charAt(0).toUpperCase() + subscription.plan.slice(1);\n        Alert.default.alert('Worker Limit Reached', `Your ${planName} plan allows up to ${subscription.plan === 'basic' ? '10' : subscription.plan === 'standard' ? '50' : 'unlimited'} workers. Upgrade to add more workers.`, [{\n          text: 'Cancel',\n          style: 'cancel'\n        }, {\n          text: 'Upgrade',\n          onPress: () => router.push('/plans')\n        }]);\n        return;\n      }\n\n      // For now, use a simple ID generation and add to the workers array\n      const newWorkerId = Math.max(...workers.map(w => parseInt(w.id))) + 1;\n      const newWorker = {\n        id: newWorkerId.toString(),\n        name: newWorkerData.name,\n        phone: newWorkerData.phone,\n        role: newWorkerData.role,\n        attendanceStatus: null,\n        overtime: false,\n        maxAdvanceLimit: 5000,\n        // Default limit\n        hidden: false,\n        todayAdvanceTotal: 0,\n        dailyRate: newWorkerData.dailyRate,\n        site: newWorkerData.site || siteParam || 'All Sites'\n      };\n      const updatedWorkers = [...workers, newWorker];\n      setWorkers(updatedWorkers);\n\n      // Save to AsyncStorage\n      await (0, _libStorage.saveWorkersList)(updatedWorkers);\n      Alert.default.alert('Success', `Worker ${newWorkerData.name} added successfully!`);\n    };\n\n    // Get badge style based on status\n    const getBadgeStyle = (worker, badgeType) => {\n      const statusMap = {\n        P: 'present',\n        H: 'halfday',\n        A: 'absent'\n      };\n      const isActive = worker.attendanceStatus === statusMap[badgeType];\n      if (isActive) {\n        if (badgeType === 'P') return {\n          backgroundColor: '#4CAF50',\n          color: '#FFFFFF'\n        };\n        if (badgeType === 'H') return {\n          backgroundColor: '#FFC107',\n          color: '#FFFFFF'\n        };\n        if (badgeType === 'A') return {\n          backgroundColor: '#F44336',\n          color: '#FFFFFF'\n        };\n      }\n      return {\n        backgroundColor: '#E0E0E0',\n        color: '#757575'\n      };\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n      style: styles.safeArea,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(StatusBar.default, {\n        barStyle: \"dark-content\",\n        backgroundColor: \"#FFFFFF\"\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 636,\n        columnNumber: 7\n      }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.container,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.header,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n            onPress: () => router.back(),\n            style: styles.backButton,\n            children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.MaterialIcons, {\n              name: \"arrow-back\",\n              size: 24,\n              color: \"#1A237E\"\n            }, void 0